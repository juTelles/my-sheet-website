let dropdownMenu = () => {
  let navLinks = document.getElementById("nav-links-mobile");
  let menuButton = document.getElementById("menu-button");

  if (window.matchMedia("(max-width: 1175px)").matches && navLinks.style.display === "none") {
    navLinks.style.display = "flex";
    menuButton.style.backgroundColor = "#FF739F"
  } else {
    navLinks.style.display ="none";
    menuButton.style.backgroundColor = "#BF436A"
  }
};
